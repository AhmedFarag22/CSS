 animation-play-state: running;/*التغيير هيشتغل*/


}

@keyframes up-and-down {/*هيقل و ضوحه و يرجع يظهر تاني*/
    to {
        opacity: 0.2;
        
    }
}

@keyframes spin {
    0% {
        transform: rotate(0deg);
    }

    5% {
        transform: rotate(360deg);
    }
}

@keyframes change-color {/*animation لتغيير اللون من الاحمر الي الازرق*/
    from {
        background-color: red;
    }

    to {
        background-color: blue;
    }
}

@keyframes change-color {/*animation  لتغيير اللون من الاحمر الي الازرق الي لاحمر */
    0% {
        background-color: red;
    }

    50% {
        background-color: blue;
    }

    100% {
        background-color: red;
    }

}

